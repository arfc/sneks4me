#
# nek parameter file
#
[GENERAL]
stopAt = endTime
endTime  = 120

variableDT = yes
targetCFL = 0.5
#dt = 0.0005
timeStepper = bdf2

filtering = explicit
filterWeight = 0.05
filterCutoffRatio = 2.0

writeControl = runTime
writeInterval = 1.0

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-5
residualProj = yes

[VELOCITY]
residualTol = 1e-6
density = 1
viscosity = -1000.38

[TEMPERATURE]
conjugateHeatTransfer = yes
rhoCp = 1.0
conductivity = -10704.12874
residualTol = 1e-06
