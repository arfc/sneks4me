#
# nek parameter file
#
[GENERAL]
stopAt = endTime
endTime  = 0.0005

#variableDT = yes
#targetCFL = 0.4
dt = 0.0001
timeStepper = bdf2

writeControl = runTime
writeInterval = 0.0001

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-5
residualProj = yes

[VELOCITY]
residualTol = 1e-8
density = 1
viscosity = -1000.38

[TEMPERATURE]

conjugateHeatTransfer = yes

rhoCp = 1.0
conductivity = -10704.12874
residualTol = 1e-06
